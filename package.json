{
    "name": "james-toohey",
    "private": true,
    "description": "James Toohey's portfolio and blog",
    "version": "0.1.0",
    "author": "James Toohey <james@toohey.dev>",
    "scripts": {
        "add-cname": "echo toohey.dev > public/CNAME",
        "clean": "gatsby clean",
        "dev": "gatsby develop",
        "build": "gatsby build && npm run add-cname",
        "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
        "lint": "eslint --ext .tsx,.ts ./",
        "lint:fix": "npm run lint -- --fix",
        "serve": "gatsby serve",
        "deploy": "gatsby build && gh-pages -d public -b master",
        "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
    },
    "dependencies": {
        "@radix-ui/react-dialog": "0.1.7",
        "@types/react-helmet": "6.1.4",
        "framer-motion": "^6.3.3",
        "gatsby": "3.14.3",
        "gatsby-image": "3.11.0",
        "gatsby-plugin-catch-links": "3.14.0",
        "gatsby-plugin-google-analytics": "3.14.0",
        "gatsby-plugin-manifest": "3.14.0",
        "gatsby-plugin-offline": "3.10.2",
        "gatsby-plugin-react-helmet": "3.10.0",
        "gatsby-plugin-sass": "3.2.0",
        "gatsby-plugin-sharp": "3.14.1",
        "gatsby-plugin-sitemap": "3.3.0",
        "gatsby-plugin-typescript": "3.14.0",
        "gatsby-remark-autolink-headers": "3.2.0",
        "gatsby-remark-prismjs": "3.13.0",
        "gatsby-source-filesystem": "3.14.0",
        "gatsby-transformer-remark": "3.2.0",
        "gatsby-transformer-sharp": "3.14.0",
        "node-sass": "6.0.1",
        "prismjs": "1.25.0",
        "prop-types": "15.7.2",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-helmet": "6.1.0",
        "react-transition-group": "4.4.1"
    },
    "devDependencies": {
        "@types/react-dom": "^17.0.11",
        "@types/react-transition-group": "^4.4.0",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-react-hooks": "^4.5.0",
        "gh-pages": "3.2.3",
        "prettier": "2.6.2",
        "typescript": "4.4.4"
    },
    "license": "MIT"
}
